<ul class="treeview-menu">
  <% object.each do |item| %>
    <% if can? :read, item %>
      <li>
        <%= link_to "/#{@namespace}/#{item.table_name}" do %>
          <i class="fa fa-circle-o"></i>
          <%= t "layouts.#{item.table_name}" %>
        <% end %>
      </li>
    <% end %>
  <% end %>
  <li>
    <%= link_to admin_imports_path do %>
      <i class="fa fa-circle-o"></i>
      <%= t "import_data.import_questions" %>
    <% end %>
  </li>
</ul>
